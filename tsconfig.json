{
	"exclude": [
		"node_modules"
	],
	"compilerOptions": {
		"module": "CommonJS",
		"rootDir": "src",
		"outDir": "./bin",
		"lib": ["ES2024"],
		"target": "ES2024",
		"sourceMap": true,
		"esModuleInterop": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noPropertyAccessFromIndexSignature": false,
		"strict": true
	}
}